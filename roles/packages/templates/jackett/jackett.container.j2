[Container]
Image=docker.io/linuxserver/jackett:latest
Label=io.containers.autoupdate=registry
PublishPort=4001:4001
PublishPort=127.0.0.1:8080:8080
PublishPort=127.0.0.1:5001:5001
RemapUsers=keep-id
Volume=/var/lib/jackett:/config:rw
Volume={{ large_dirs }}/downloads:rw

[Service]
SuccessExitStatus=0

[Install]
WantedBy=default.target