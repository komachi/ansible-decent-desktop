general:
    debug: False
    instance_name: "searx"

search:
    safe_search: 0
    autocomplete: ""
    default_lang: ""
    ban_time_on_fail: 5
    max_ban_time_on_fail: 120

server:
    port: 8888
    bind_address: "127.0.0.1"
    secret_key: "{{ lookup('password', 'credentials/searx_secret_key length=25') }}"
    base_url: False
    image_proxy: False
    http_protocol_version: "1.1"

ui:
    static_path: ""
    templates_path: ""
    default_theme: oscar
    default_locale: ""
    theme_args:
        oscar_style: logicodev-dark
    categories_order:
      - general
      - images
      - videos
      - science
      - news
      - files
      - it
      - onions
preferences:
    lock:
      - image_proxy

enabled_plugins:
   - "HTTPS rewrite"
   - "Hash plugin"
   - "Open Access DOI rewrite"
   - "Self Informations"
   - "Search on category select"
   - "Tracker URL remover"

outgoing:
    request_timeout: 2.0
    max_request_timeout: 15.0
    useragent_suffix: ""
    pool_connections: 100
    pool_maxsize: 50
    extra_proxy_timeout: 10.0

engines:
  - name: ddg definitions
    engine: duckduckgo_definitions
    shortcut: ddd
    weight: 2
    disabled: False
    url: http://api.3g2upl4pq6kufc4m.onion/?{query}&format=json&pretty=0&no_redirect=1&d=1
    using_tor_proxy: True
    timeout: 5.0
    categories: general
    proxies:
       http: socks5h://searx:searx@127.0.0.1:9050
       https: socks5h://searx:searx@127.0.0.1:9050

  - name: wikipedia
    engine: wikipedia
    shortcut: wp
    categories: general
    base_url: 'https://{language}.wikipedia.org/'

  - name: wikidata
    engine: wikidata
    shortcut: wd
    timeout: 3.0
    weight: 2
    categories: general

  - name: duckduckgo
    engine: duckduckgo
    shortcut: ddg
    disabled: False
    url: https://3g2upl4pq6kufc4m.onion/html
    timeout: 5.0
    using_tor_proxy: True
    proxies:
       http: socks5h://searx:searx@127.0.0.1:9050
       https: socks5h://searx:searx@127.0.0.1:9050

  - name: startpage
    engine: startpage
    shortcut: sp
    timeout: 6.0
    disabled: False
    categories: general

  - name: qwant
    engine: qwant
    shortcut: qw
    categories: general
    disabled: False

  - name: currency
    engine: currency_convert
    categories: general
    shortcut: cc

  - name: bing images
    engine: bing_images
    shortcut: bii

  - name: duckduckgo images
    engine: duckduckgo_images
    shortcut: ddi
    timeout: 5.0
    disabled: False
    images_url: https://3g2upl4pq6kufc4m.onion/i.js?{query}&s={offset}&p={safesearch}&o=json&vqd={vqd}
    site_url: https://3g2upl4pq6kufc4m.onion/?{query}&iar=images&iax=1&ia=images
    using_tor_proxy: True
    proxies:
       http: socks5h://searx:searx@127.0.0.1:9050
       https: socks5h://searx:searx@127.0.0.1:9050

  - name: qwant images
    engine: qwant
    shortcut: qwi
    categories: images

  - name: flickr
    categories: images
    shortcut: fl
    engine: flickr_noapi

  - name: google news
    engine: google_news
    shortcut: gon
    categories: news

  - name: yahoo news
    engine: yahoo_news
    shortcut: yhn
    categories: news

  - name: bing news
    engine: bing_news
    shortcut: bin
    categories: news

  - name: qwant news
    engine: qwant
    shortcut: qwn
    categories: news

  - name: arch linux wiki
    engine: archlinux
    shortcut: al
    categories: it

  - name: npm
    engine: json_engine
    paging: True
    search_url: https://api.npms.io/v2/search?q={query}&size=25&from={pageno}
    results_query: results
    url_query: package/links/npm
    title_query: package/name
    content_query: package/description
    page_size: 25
    categories: it
    disabled: False
    timeout: 5.0
    shortcut: npm
    categories: it

  - name: github
    engine: github
    shortcut: gh
    categories: it

  - name: stackoverflow
    engine: stackoverflow
    shortcut: st
    categories: it

  - name: pubmed
    engine: pubmed
    shortcut: pub
    categories: science
    timeout: 3.0

  - name: arxiv
    engine: arxiv
    shortcut: arx
    categories: science
    timeout: 4.0

  - name: library genesis
    engine: xpath
    url_xpath: //a[contains(@href,"bookfi.net")]/@href
    title_xpath: //a[contains(@href,"book/")]/text()[1]
    content_xpath: //td/a[1][contains(@href,"=author")]/text()
    timeout: 7.0
    disabled: False
    shortcut: lg
    search_url: http://genotypeinczgrxr.onion/search.php?req={query}
    using_tor_proxy: True
    categories: science
    proxies:
       http: socks5h://searx:searx@127.0.0.1:9050
       https: socks5h://searx:searx@127.0.0.1:9050

  - name: soundcloud
    engine: soundcloud
    shortcut: sc
    categories: music

  - name: vimeo
    engine: vimeo
    shortcut: vm
    categories: videos

  - name: dailymotion
    engine: dailymotion
    shortcut: dm
    categories: videos

  - name: invidious
    engine: invidious
    base_url: 'http://w6ijuptxiku4xpnnaetxvnkc5vqcdu7mgns2u77qefoixi63vbvnpnqd.onion/'
    shortcut: iv
    timeout: 5.0
    categories: videos
    using_tor_proxy: True
    proxies:
      http: socks5h://searx:searx@127.0.0.1:9050
      https: socks5h://searx:searx@127.0.0.1:9050

  - name: sepiasearch
    engine: sepiasearch
    shortcut: sep
    categories: videos

  - name: torch
    engine: xpath
    paging: True
    search_url: http://xmh57jrknzkhv6y3ls3ubitzfqnkrwxhopf5aygthi7d6rplyvk3noyd.onion/cgi-bin/omega/omega?P={query}&DEFAULTOP=and
    results_xpath: //table//tr
    url_xpath: ./td[2]/a
    title_xpath: ./td[2]/b
    content_xpath: ./td[2]/small
    categories: onions
    shortcut: tch
    using_tor_proxy: True
    proxies:
      http: socks5h://searx:searx@127.0.0.1:9050
      https: socks5h://searx:searx@127.0.0.1:9050

  - name: ahmia
    engine: ahmia
    categories: onions
    shortcut: ah
    using_tor_proxy: True
    proxies:
      http: socks5h://searx:searx@127.0.0.1:9050
      https: socks5h://searx:searx@127.0.0.1:9050

  - name: not evil
    engine: not_evil
    shortcut: ne
    using_tor_proxy: True
    proxies:
      http: socks5h://searx:searx@127.0.0.1:9050
      https: socks5h://searx:searx@127.0.0.1:9050

