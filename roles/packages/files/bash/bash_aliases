alias bc='bc -lq'
alias diff='colordiff'
alias wget='wget -c --show-progress'
alias cpv='rsync -ah --info=progress2'
alias less='less -r'
alias sudo='doas'
alias path='echo $PATH'
alias ..='cd ..'
alias l='lsd'
alias df='duf'
alias cat='batcat'
alias imv='imv-wayland'
alias q="qalc -i"