{
  "editor.tabSize": 2,
  "editor.fontFamily": "Hack, 'Noto Sans Mono Black', 'Noto Color Emoji'",
  "editor.minimap.enabled": false,
  "editor.detectIndentation": true,
  "editor.smoothScrolling": true,
  "editor.fontLigatures": true,
  "editor.guides.bracketPairs": "active",
  "editor.tabCompletion": "on",
  "editor.tabFocusMode": false,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "editor.renderWhitespace": "trailing",
  "editor.accessibilitySupport": "off",
  "editor.matchBrackets": "near",
  "files.trimTrailingWhitespace": true,
  "workbench.colorTheme": "Adwaita Dark & default syntax highlighting",
  "workbench.editor.enablePreview": false,
  "workbench.sideBar.location": "left",
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.editorActionsLocation": "default",
  "workbench.layoutControl.enabled": false,
  "workbench.enableExperiments": false,
  "window.commandCenter": false,
  "window.closeWhenEmpty": true,
  "window.openFilesInNewWindow": "on",
  "window.titleBarStyle": "native",
  "window.menuBarVisibility": "hidden",
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.ignorePaths": [
    "node_modules/**",
    "dist/**"
  ],
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[python]": {
    "editor.tabSize": 4,
    "python.formatting.provider": "black",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.flake8Enabled": true
  },
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.renderWhitespace": "all",
    "editor.acceptSuggestionOnEnter": "off"
  },
  "eslint.format.enable": true,
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "shellcheck.disableVersionCheck": true,
  "yaml.extension.recommendations": false,
  "go.toolsManagement.checkForUpdates": "off",
  "path-intellisense.showOnAbsoluteSlash": false,
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "extensions.webWorker": true,
  "extensions.ignoreRecommendations": true,
  "telemetry.telemetryLevel": "off",
  "telemetry.enableErrorTelemetry": false,
  "vsintellicode.features.python.deepLearning": "disabled",
  "redhat.telemetry.enabled": false,
  "gitlens.telemetry.enabled": false,
  "vuedx.telemetry": false,
  "ask.telemetryEnabled": "Disabled",
  "textmarker.enableTelemetry": false,
  "sqltools.telemetry": false,
  "platformio.enableTelemetry": false,
  "AREPL.telemetry": false,
  "dotnet.enableTelemetry": false,
  "gitlens.advanced.telemetry.enabled": false,
  "sonarlint.disableTelemetry": true,
  "pinterest.enableTelemetry": false,
  "phabricator.enableTelemetry": false,
  "intelephense.telemetry.enabled": false,
  "mdb.sendTelemetry": false,
  "poshProTools.enableTelemetry": false,
  "vscode-mysql.enableTelemetry": false,
  "azurermtools.enableTelemetry": false,
  "cloudcode.enableTelemetry": false,
  "docker-explorer.enableTelemetry": false,
  "cortex-debug.enableTelemetry": false,
  "angularConsole.enableTelemetry": false,
  "aws.telemetry": false,
  "githubPullRequests.telemetry.enabled": false,
  "dotnet-test-explorer.enableTelemetry": false,
  "partialDiff.enableTelemetry": false,
  "code-runner.enableAppInsights": false,
  "terraform.telemetry.enabled": false,
  "edi-x12.telemetry.disabled": true,
  "eppz-code.disableAnalytics": true,
  "codeQL.telemetry.enableTelemetry": false,
  "module-debug.enableTelemetry": false,
  "ecdc.collectTelemetry": false,
  "docsYaml.telemetry.enableTelemetry": false,
  "idf.telemetry": false,
  "julia.enableTelemetry": false,
  "julia.enableCrashReporter": false,
  "printcode.disableTelemetry": false,
  "rest-client.enableTelemetry": false,
  "serverlessIDE.telemetry.enableTelemetry": false,
  "terminal.enableAppInsights": false,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.productIconTheme": "adwaita",
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[css]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.virtualRepositories.enabled": false,
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.hovers.avatars": false,
