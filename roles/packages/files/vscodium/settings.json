  "gitlens.views.drafts.avatars": false,
  "gitlens.views.patchDetails.avatars": false,
  "gitlens.views.workspaces.pullRequests.enabled": false,
  "gitlens.views.workspaces.pullRequests.showForBranches": false,
  "gitlens.views.workspaces.pullRequests.showForCommits": false,
  "gitlens.views.workspaces.avatars": false,
  "gitlens.gitCommands.avatars": false,
  "gitlens.integrations.enabled": false,
  "gitlens.cloudIntegrations.enabled": false,
  "gitlens.liveshare.enabled": false,
  "gitlens.liveshare.allowGuestAccess": false,
  "gitlens.ai.generateCommitMessage.enabled": false,
  "gitlens.blame.avatars": false,
  "gitlens.views.lineHistory.avatars": false,
  "gitlens.views.branches.avatars": false,
  "gitlens.views.worktrees.avatars": false,
  "gitlens.views.contributors.avatars": false,
  "gitlens.views.searchAndCompare.avatars": false,
  "gitlens.views.commits.pullRequests.showForCommits": false,
  "gitlens.views.workspaces.showBranches": false,
  "ansible.lightspeed.suggestions.enabled": false,
  "ansible.lightspeed.enabled": false,
  "docker.images.checkForOutdatedImages": false,
  "python.experiments.enabled": false,
  "openapi.platformAuditRuntime": "cli",
  "stylelint.reportNeedlessDisables": true,
  "stylelint.reportDescriptionlessDisables": true,
  "stylelint.reportInvalidScopeDisables": true,
  "eslint.useFlatConfig": true
}